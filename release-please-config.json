{
  "packages": {
    ".": {
      "package-name": "foundry-mcp-server",
      "release-type": "node",
      "extra-files": [
        {
          "type": "json",
          "path": "package.json",
          "jsonpath": "$.version"
        }
      ]
    },
    "foundry-local-rest-api": {
      "package-name": "foundry-local-rest-api-module",
      "release-type": "simple",
      "extra-files": [
        {
          "type": "json",
          "path": "foundry-local-rest-api/module.json",
          "jsonpath": "$.version"
        }
      ]
    }
  },
  "plugins": [
    "node-workspace",
    {
      "type": "linked-versions",
      "groupName": "foundry-mcp-packages",
      "components": ["foundry-mcp-server", "foundry-local-rest-api-module"]
    }
  ]
}
