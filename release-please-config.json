{
  "packages": {
    ".": {
      "component": "foundry-mcp-server",
      "extra-files": [
        {
          "type": "json",
          "path": "foundry-local-rest-api/module.json",
          "jsonpath": "$.version"
        }
      ]
    },
    "foundry-local-rest-api": {
      "component": "foundry-local-rest-api",
      "release-type": "simple",
      "extra-files": [
        {
          "type": "json",
          "path": "foundry-local-rest-api/module.json",
          "jsonpath": "$.version"
        }
      ]
    }
  },
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "foundry-mcp-packages",
      "components": ["foundry-mcp-server", "foundry-local-rest-api"]
    }
  ]
}
