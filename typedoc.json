{
  "entryPoints": ["src/index.ts", "src/foundry/client.ts", "src/config/index.ts", "src/foundry/types.ts", "src/utils/logger.ts"],
  "out": "docs",
  "exclude": ["**/*.test.ts", "**/node_modules/**", "src/tools/**", "src/character/**", "src/combat/**", "src/resources/**"],
  "name": "FoundryVTT MCP Server",
  "readme": "README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "categorizeByGroup": true,
  "groupOrder": ["Classes", "Interfaces", "Functions", "Variables"],
  "sort": ["source-order"],
  "theme": "default",
  "hideGenerator": false,
  "searchInComments": true,
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  }
}
